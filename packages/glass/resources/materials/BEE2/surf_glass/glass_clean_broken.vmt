ShatteredGlass
{
	"$basetexture" "glass/glasswindow_frosted_002b"
	// Adds the caution stripes in.
	$detail "BEE2/surf_glass/glass_shatter_stripe"
	$surfaceprop "glass"

	// The edge textures use this for their transparency,
	// we have to have it.
	$additive 1
	
    // 1 = fullbright, 0 = normal
	$unlitfactor 0

	"%keywords" "BEE2 P1"

	"$nocull" 1
	"Proxies"
	{
		"BreakableSurface"
		{
		// Sets $basetexture automatically.
		}
	}
}
